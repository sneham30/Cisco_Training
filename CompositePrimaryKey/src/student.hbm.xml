<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Student" table="STUDENTPK">
        <composite-id>
         	<key-property name="studentId" column="ID"  />
			<key-property name="StudentName" column="studentname" length="30" />
		</composite-id>
        
        <property name="age" type="int">
        	<column name="age" />
        </property>
       
    </class>
</hibernate-mapping>
